{
  "$schema": "./node_modules/@types/json-schema/package.json",
  "name": "daily-dash",
  "version": "1.0.0",
  "description": "Your all-in-one dashboard for news, weather, tasks, events, and commuting.",
  "main": "./src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "nodemon -L ./src/server.ts",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier . --check",
    "docker:up": "docker compose --env-file ./secrets/.env up",
    "docker:down": "docker-compose --env-file ./secrets/.env down && docker rmi daily-dash postgres",
    "migrate": "knex --knexfile ./out/db/knexfile.js migrate:latest",
    "seed": "knex --knexfile ./out/db/knexfile.js seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hgorges/DailyDash.git"
  },
  "keywords": [
    "dashboard",
    "news",
    "weather",
    "todos",
    "events",
    "calendar",
    "commuting"
  ],
  "author": "CryptoSpaceDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hgorges/DailyDash.git/issues"
  },
  "homepage": "https://github.com/hgorges/DailyDash.git#readme",
  "devDependencies": {
    "@doist/todoist-api-typescript": "^3.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.18",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.11",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.10.7",
    "@types/pg": "^8.10.5",
    "@types/serve-favicon": "^2.5.7",
    "@types/supertest": "^2.0.16",
    "@types/xml-js": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@googleapis/calendar": "^9.7.0",
    "@googleapis/docs": "^3.0.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "google-auth-library": "^9.6.3",
    "googleapis": "^30.0.0",
    "googleapis-common": "^7.0.1",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "rss-parser": "^3.13.0",
    "serve-favicon": "^2.5.0",
    "xml-js": "^1.6.11"
  }
}
